    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service-category.service-category",
      "inversedBy": "services"
    },
    "type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service-type.service-type",
      "inversedBy": "services"
    },
    "agency_services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::agency-service.agency-service",
      "mappedBy": "service"
    },
    "serviceImage": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "confirmed": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "required": true
    }
